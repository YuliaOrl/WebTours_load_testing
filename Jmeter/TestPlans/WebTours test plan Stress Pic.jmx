// Граничные значения случайной даты 
def startDateDeparture = LocalDate.of(2025, 8, 20),
	endDateDeparture = LocalDate.of(2025, 9, 3),
	startDateReturn = LocalDate.of(2025, 9, 20),
	endDateReturn = LocalDate.of(2025, 10, 20)

// Генерация первой случайной даты
def randomEpochDay1 = ThreadLocalRandom.current().nextLong(startDateDeparture.toEpochDay(), endDateDeparture.toEpochDay() + 1),
	randomDate1 = LocalDate.ofEpochDay(randomEpochDay1)

// Генерация второй случайной даты, которая будет позже первой
def randomEpochDay2 = ThreadLocalRandom.current().nextLong(startDateReturn.toEpochDay(), endDateReturn.toEpochDay() + 1),
	randomDate2 = LocalDate.ofEpochDay(randomEpochDay2)

def departureDate = randomDate1.format(DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;)),
	returnDate = randomDate2.format(DateTimeFormatter.ofPattern(&quot;MM/dd/yyyy&quot;))

vars.put(&quot;departureDate5&quot;, departureDate)
vars.put(&quot;returnDate5&quot;, returnDate)


