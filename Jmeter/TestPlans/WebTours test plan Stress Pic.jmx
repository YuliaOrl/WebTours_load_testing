                  <stringProp name="script">// Получение HTML-ответа предыдущего запроса
def htmlResponse = prev.getResponseDataAsString()

// Регулярное выражение для поиска значений flightID
def pattern = /name=&quot;flightID&quot;\s+value=&quot;([A-Za-z0-9\-]+)&quot;/

// Поиск всех совпадений
def matcher = htmlResponse =~ pattern

def flightIDs = []

// Извлечение значений из совпадений
matcher.each { match -&gt;
    flightIDs.add(match[1]) // match[1] содержит значение внутри скобок регулярного выражения
}

// Выбор случайного количества билетов для удаления (от 1 до 3)
def random = new Random()
//def numberOfFlightsToRemove = Math.min(random.nextInt(3) + 1, flightIDs.size() - 1)
def numberOfFlightsToRemove = 1

def countFlightsAfter = flightIDs.size() - numberOfFlightsToRemove // Количество броней после удаления, которое должно быть в итоге проверено

// flightIDs разделяем на удаляемые и оставшиеся
def selectedFlightIDs = flightIDs.take(numberOfFlightsToRemove) // Удаляемые
def remainingFlightIDs = flightIDs.drop(numberOfFlightsToRemove) // Оставшиеся

// Граница для multipart/form-data
def boundary = &quot;------WebKitFormBoundaryNYbXDn5Z3B9AKgFC&quot;

// Формирование тела запроса
def requestBody = new StringBuilder()

// Добавление выбранных flightID (удаляемых) в тело запроса
 //Добавление текста &quot;on&quot; n раз
(1..numberOfFlightsToRemove).each { index -&gt;
    requestBody.append(&quot;on&quot;)
}

requestBody.append(&quot;5523490000-38-CC7&quot;)

selectedFlightIDs.eachWithIndex { flightID, index -&gt;
    requestBody.append(&quot;${boundary}\r\n&quot;)
    requestBody.append(&apos;Content-Disposition: form-data; name=&quot;&apos; + (index + 1) + &apos;&quot;\r\n\r\n&apos;)
    requestBody.append(&quot;on\r\n&quot;)

    requestBody.append(&quot;${boundary}\r\n&quot;)
    requestBody.append(&apos;Content-Disposition: form-data; name=&quot;flightID&quot;\r\n\r\n&apos;)
    requestBody.append(&quot;${flightID}\r\n&quot;)
}

// Добавление оставшихся flightID (не удаляемых) в тело запроса
remainingFlightIDs.eachWithIndex { flightID, index -&gt;
    requestBody.append(&quot;${boundary}\r\n&quot;)
    requestBody.append(&apos;Content-Disposition: form-data; name=&quot;flightID&quot;\r\n\r\n&apos;)
    requestBody.append(&quot;${flightID}\r\n&quot;)
}

// Добавление координат кнопки &quot;removeFlights&quot;
requestBody.append(&quot;${boundary}\r\n&quot;)
requestBody.append(&apos;Content-Disposition: form-data; name=&quot;removeFlights.x&quot;\r\n\r\n&apos;)
requestBody.append(&quot;68\r\n&quot;)

requestBody.append(&quot;${boundary}\r\n&quot;)
requestBody.append(&apos;Content-Disposition: form-data; name=&quot;removeFlights.y&quot;\r\n\r\n&apos;)
requestBody.append(&quot;11\r\n&quot;)

// Добавление .cgifields
(1..flightIDs.size()).each { index -&gt;
    requestBody.append(&quot;${boundary}\r\n&quot;)
    requestBody.append(&apos;Content-Disposition: form-data; name=&quot;.cgifields&quot;\r\n\r\n&apos;)
    requestBody.append(&quot;${index}\r\n&quot;)
}

// Закрытие границы
requestBody.append(&quot;${boundary}--\r\n&quot;)

vars.put(&quot;requestBodyRandom&quot;, requestBody.toString())

vars.put(&quot;countFlightsAfter&quot;, countFlightsAfter.toString())
</stringProp>
