              <JSR223PostProcessor guiclass="TestBeanGUI" testclass="JSR223PostProcessor" testname="JSR223 PostProcessor Create users data" enabled="true">
                <stringProp name="cacheKey">true</stringProp>
                <stringProp name="filename"></stringProp>
                <stringProp name="parameters"></stringProp>
                <stringProp name="script">import groovy.json.JsonSlurper
import java.time.YearMonth
import java.time.format.DateTimeFormatter

def userDataJson = vars.get(&quot;userDataJson&quot;) // Извлекаем исходную JSON-строку с данными пользователей, полученную по API

if (userDataJson != null) {
    def jsonSlurper = new JsonSlurper()
    def users = jsonSlurper.parseText(userDataJson) // Преобразуем JSON-строку в объект Groovy users   

    def random = new Random()

    users.results.each { element -&gt;
    def firstname = element.name.first,
		lastname = element.name.last,
		username = firstname.toLowerCase() + &quot;${__RandomString(3,abcdefghijklmnopkqrstuvwxyz)}&quot;,
		password = element.login.password,
		street = element.location.street.name.replace(&apos;-&apos;, &apos; &apos;),
		city = element.location.city,
		state = element.location.state,
		zip = element.location.postcode.toString(),
		city_state_zip = city + &quot;/&quot; + state + &quot;/&quot; + zip,
		passenger = firstname + &quot; &quot; + lastname,
		card = (1..16).collect { random.nextInt(10) }.join().toString(), // Генерируем строку из 16 случайных чисел
		randomDate = YearMonth.of(2026 + random.nextInt(11), 1 + random.nextInt(12)),
		expDate = randomDate.format(DateTimeFormatter.ofPattern(&quot;MM/yy&quot;))

    vars.put(&quot;username&quot;, username)
    vars.put(&quot;password&quot;, password)
    vars.put(&quot;firstname&quot;, firstname)
    vars.put(&quot;lastname&quot;, lastname)
    vars.put(&quot;street&quot;, street)
    vars.put(&quot;city_state_zip&quot;, city_state_zip)
    vars.put(&quot;passenger&quot;, passenger)
    vars.put(&quot;card&quot;, card)
    vars.put(&quot;expDate&quot;, expDate.toString())

    if (username != null) {
        vars.put(&quot;userExist&quot;, &quot;1&quot;) // Присваиваем переменной userExist значение 1 для проверки в If Controller Rigister users для регистрации  
        }
    }
}
</stringProp>
